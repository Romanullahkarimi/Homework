class Main {
 public static void main(String[] args) {
   Scanner scanner=new Scanner(System.in);
   System.out.println("Print one number");
   int a=scanner.nextInt();

   for (int x=0; x<a; x++){
     System.out.print(x+" ");
   }
   scanner.close();
    }
}
