import java.util.Scanner;
class Main{
public static void main (String[] args){
  Scanner scanner=new Scanner(System.in);
  System.out.println("Print one number");
  int end=scanner.nextInt();

  for (int a=0; a<end*2; a++){
    System.out.print(a+" ");
  }
  scanner.close();
}
}
