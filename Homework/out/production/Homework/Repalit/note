import java.util.Scanner;

class Main {
  public static void main(String[] args) {
 Scanner scanner=new Scanner(System.in);
    System.out.println("Print one number");
    int x=scanner.nextInt();
    for (int a=x; a>=0; a--){
     if (a==x){
       continue;
     } System.out.print(a+" ");
    }
    }
}
